package {	import flash.display.MovieClip;	import flash.events.MouseEvent;	//public class RabidMole	public class RabidMole extends MovieClip	{		//declare variable  rabidValue of type int with value 5		var rabidValue:int = 5;				//declare variable clicking of type int		var clicking:int;		public function RabidMole():void		{			// constructor code			//set clicking = 0			//add a listener and invoke swipe when mouse is clicked			clicking = 0;			this.addEventListener(MouseEvent.CLICK, swipe);		}		// Instance method that removes squishle		private function swipe(e : MouseEvent):void		{			//update and store value of clicking 			clicking +=  1;						//if the mole is clicked 3 times, remove the mole			if (clicking ==3)			{				//tell the parent object to remove this instance				this.parent.removeChild( this );			}		}		public function pointValue():int		{			//if a mole is clicked 3 times, return rabid Value (5)			//else, return 0			if (clicking ==3)			{				return rabidValue;			}			else			{				return 0;			}		}	}}